
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


// entidade de login no caso o usu√°rio, utilizando mecaniscmo de email (unico)
model User {
  id Int @id @default(autoincrement())

  email     String @unique
  passaword String

  name String
}